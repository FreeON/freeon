	'TS4/CommErr_0.10D-05.dat'  using 1:7 title "TS4(10^{-6})" with linespoints,\
	'PM/CommErr_0.10D-02.dat'  using 1:7 title "PM(10^{-3})" with linespoints,\
	'PM/CommErr_0.10D-03.dat'  using 1:7 title "PM(10^{-4})" with linespoints,\
	'PM/CommErr_0.10D-04.dat'  using 1:7 title "PM(10^{-5})" with linespoints,\
	'PM/CommErr_0.10D-05.dat'  using 1:7 title "PM(10^{-6})" with linespoints
