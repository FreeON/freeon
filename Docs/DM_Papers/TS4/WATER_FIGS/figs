################################################################################
set output "ConvergedWaterErrors.eps"
set size square 1.2, 1.6
set xlabel "Water Molecules, N_{H_2O}"
set ylabel "Energy per Water" -.5,0
set xrange [10:150]
plot	'ConvergedEnergies.dat'  using 1:2 title "RH" with linespoints,\
	'ConvergedEnergies.dat'  using 1:3 title "PM(10^{-4})" with linespoints,\
	'ConvergedEnergies.dat'  using 1:4 title "TS4(10^{-4})" with linespoints
################################################################################
set output "WaterTimings.eps"
set size square 1.2, 1.6
set xlabel "Water Molecules, N_{H_2O}"
set ylabel "CPU Seconds per SCF cycle" -.5,0
set xrange [10:150]
set yrange [0:1600]
plot	'ConvergedEnergies.dat'  using 1:5 title "RH" with linespoints,\
	'ConvergedEnergies.dat'  using 1:6 title "PM(10^{-4})" with linespoints,\
	'ConvergedEnergies.dat'  using 1:7 title "TS4(10^{-4})" with linespoints
################################################################################
