Cell[BoxData[
    \(omega2[lambda_, i_, j_, k_, K_List] := 
      Module[\ {Khat}, \[IndentingNewLine]Khat = 
          K/Sqrt[K . K]; \[IndentingNewLine]tk = 
          ArcCos[K[\([3]\)]/Sqrt[K . K]]; \[IndentingNewLine]pk = 
          ArcTan[K[\([1]\)], K[\([2]\)]]; \[IndentingNewLine]sum = 
          0; \[IndentingNewLine]Do[\[IndentingNewLine]\(sum = 
              sum + Z1[lambda, mu, tk, pk]\ *\ 
                  Integrate[\ 
                    Z1[lambda, mu, t, p]\ \ Sin[
                        t]\ \((\ Cos[p]\ Sin[t]\ )\)^\((i)\)\ \ \((\ 
                          Sin[p]\ Sin[t])\)^\((j)\)\ \ \((Cos[
                            t])\)^\((k)\)\ , {p, 0, 2\ Pi}, {t, 
                      0, \ \ Pi}];\)\ \[IndentingNewLine], {mu, \(-lambda\), 
            lambda}]; \[IndentingNewLine]sum]\)], "Input"],

Cell[BoxData[
    \(Z2[l_, m_, xx_, yy_, zz_] := 
      Module[\ {prefactor, em, x, y, z}, \[IndentingNewLine]em = 
          Abs[m]; \[IndentingNewLine]prefactor = 
          Sqrt[\((2\ l + 
                  1)\) \(\((l - 
                      em)\)!\)/\((2\ \ Pi \(\((l + 
                          em)\)!\))\)]; \[IndentingNewLine]r = 
          Sqrt[xx^2 + yy^2 + zz^2]; \[IndentingNewLine]x = 
          xx; \[IndentingNewLine]y = yy; \[IndentingNewLine]z = 
          zz; \[IndentingNewLine]dp = 
          D[LegendreP[l, u], {u, em}] /. u \[Rule] z; \[IndentingNewLine]If[
          m > 0, \ midfact = 
            prefactor*\((1 - I + \((1 + I)\))\)/
                2\ \ \((\((x + \ I\ y)\)^em + \((x - I\ \ y)\)^em)\)/
                2]; \[IndentingNewLine]If[m \[Equal] 0, 
          midfact = 
            Sqrt[\((2\ l + 1)\)/\((4\ \ Pi)\)]]\ ; \[IndentingNewLine]If[
          m < 0, \ midfact = 
            prefactor*\((1 - I - \((1 + I)\))\)/
                2\ \ \((\((x + \ I\ y)\)^em - \((x - I\ \ y)\)^em)\)/
                2]; \[IndentingNewLine]midfact*dp\ ]\)], "Input"],

