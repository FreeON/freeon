Notebook[{
Cell[BoxData[
    \(Z1[l_, m_, t_, p_] := 
      Module[\ {prefactor, em, tmp}, \[IndentingNewLine]em = 
          Abs[m]; \[IndentingNewLine]prefactor = 
          Sqrt[\((2\ l + 
                  1)\) \(\((l - 
                      em)\)!\)/\((2\ \ Pi \(\((l + 
                          em)\)!\))\)]; \[IndentingNewLine]If[m \[Equal] 0, 
          Return[prefactor\ \((1/Sqrt[2])\)\ LegendreP[l, 0, 
                Cos[t]]]]; \[IndentingNewLine]If[m > 0, 
          Return[prefactor\ \((\(-1\))\)^m\ Cos[em\ p] 
              LegendreP[l, em, Cos[t]]]]; \[IndentingNewLine]If[m < 0, 
          Return[\ prefactor\ \((\(-1\))\)^m\ Sin[em\ \ p] 
              LegendreP[l, em, Cos[t]]]];\[IndentingNewLine]]\)], "Input"],

Cell[BoxData[
    \(chi1[ella_List, A_List, zetaA_, ellb_List, B_List, zetaB_, ellc_, 
        C_List, zetaC_\ ] := 
      Module[{}, \[IndentingNewLine]ClearAll[x, y, z]; \[IndentingNewLine]\ 
        rC = {x, y, z} - C; \[IndentingNewLine]\ 
        rA = {x, y, z} - A; \[IndentingNewLine]\ 
        rB = {x, y, z} - B; \[IndentingNewLine]\ \ phiA = 
          rA[\([1]\)]^ella[\([1]\)]\ rA[\([2]\)]^ella[\([2]\)]\ rA[\([3]\)]^
              ella[\([3]\)]\ Exp[\(-zetaA\)\ rA . 
                  rA]; \[IndentingNewLine]\ \ phiB = 
          rB[\([1]\)]^ellb[\([1]\)]\ rB[\([2]\)]^ellb[\([2]\)]\ rB[\([3]\)]^
              ellb[\([3]\)]\ Exp[\(-zetaB\)\ rB . 
                  rB]; \[IndentingNewLine]\ \ NIntegrate[\ \ phiA\ phiB\ Sqrt[
                rC . rC]^ellc\ Exp[\(-zetaC\)\ rC . rC], {x, \(-8\), 
            8}, {y, \(-8\), 8}, {z, \(-8\), 
            8}]\[IndentingNewLine]]\)], "Input"],

Cell[BoxData[
    \(chi2[ella_List, A_List, zetaA_, ellb_List, B_List, zetaB_, ellc_, 
        C_List, zetaC_\ ] := 
      Module[{}, \[IndentingNewLine]K = \(-2\) \((zetaA\ \ \((C - A)\)\  + \ 
                zetaB\ \((C - B)\))\); \[IndentingNewLine]d = 
          Exp[\ \(-zetaA\)\ \((C - A)\) . \((C - A)\)\  - \ 
              zetaB\ \((C - B)\) . \((C - B)\)]; \[IndentingNewLine]\ 
        rC = {x, y, z} - C; \[IndentingNewLine]\ 
        rA = {x, y, z} - A; \[IndentingNewLine]\ 
        rB = {x, y, z} - B; \[IndentingNewLine]rrC = 
          Sqrt[rC . rC]; \[IndentingNewLine]\ \ phiA = 
          rA[\([1]\)]^ella[\([1]\)]\ rA[\([2]\)]^ella[\([2]\)]\ rA[\([3]\)]^
              ella[\([3]\)]\ ; \[IndentingNewLine]\ \ phiB = 
          rB[\([1]\)]^ellb[\([1]\)]\ rB[\([2]\)]^ellb[\([2]\)]\ rB[\([3]\)]^
              ellb[\([3]\)]\ ; \[IndentingNewLine]\ \ d\ NIntegrate[\ \ phiA\ \
phiB\ Sqrt[rC . rC]^
                ellc\ Exp[\(-\((zetaA + zetaB + zetaC)\)\)\ rC . rC\  + 
                  K . rC\ ], {x, \(-10\), 10}, {y, \(-10\), 10}, {z, \(-10\), 
              10}]]\)], "Input"],

Cell[BoxData[{
    \(\(\(ClearAll[j, x, M, k, r];\)\(\n\)
    \) (*\ j[l_, x_] := Sqrt[Pi/\((2\ x)\)]\ BesselJ[l + 1/2, x]; \n
