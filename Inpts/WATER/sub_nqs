rm  -f $1.nqs
echo '#\!/bin/csh'                     >  $1.nqs
echo "# QSUB -q high "                 >> $1.nqs
#echo "# QSUB -q low "                 >> $1.nqs
echo "# QSUB -r $1 -e -o out.$1"       >> $1.nqs
echo "rm $MONDO_SCRATCH/$1*"           >> $1.nqs
echo "cd `pwd`"                        >> $1.nqs
echo "mondo $1.in $1.out"              >> $1.nqs
echo "rm $MONDO_SCRATCH/$1*"           >> $1.nqs
chmod u+x $1.nqs
qsub $1.nqs







