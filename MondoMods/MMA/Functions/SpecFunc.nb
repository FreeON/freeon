Cell[BoxData[{
    \(SetOptions[FindRoot, WorkingPrecision \[Rule] 50, 
      AccuracyGoal \[Rule] 20, MaxIterations \[Rule] 40]; \ 
    Off[FindRoot::precw];\), "\[IndentingNewLine]", 
    \(\(Unprotect[D, Derivative, Function];\)\), "\[IndentingNewLine]", 
    \(\(ClearAll[F];\)\), "\[IndentingNewLine]", 
    \(\(SetAttributes[F, Listable];\)\), "\[IndentingNewLine]", 
    \(F[m_, T_] := 
      Gamma[1/2 + m, 0, T]/\((2*T^\((1/2 + m)\))\) /; 
        T \[NotEqual] 0\), "\[IndentingNewLine]", 
    \(\(F[m_, T_] := 
        1/\((2\ m + 1)\) /; T \[Equal] 0;\)\), "\[IndentingNewLine]", 
    \(\(D[F[m_, x_], x_] := \(-F[m + 1, x]\);\)\), "\[IndentingNewLine]", 
    \(\(\(\(Derivative[0, 1]\)[F]\)[m_, 
          x_] := \(-F[m + 1, x]\);\)\), "\[IndentingNewLine]", 
    \(\(\(\(Derivative[0, n_]\)[F]\)[m_, 
          x_] := \((\(-1\))\)^n\ F[m + n, x];\)\), "\[IndentingNewLine]", 
    \(\(Protect[D, Derivative, Function];\)\), "\[IndentingNewLine]", 
    \(\(GammaInTheLimit[n_, 
          T_] := \(Abs[2\ n - 1]!!\)/\((2\ \((2\ T)\)^n)\)\ Sqrt[
            Pi/T];\)\), "\[IndentingNewLine]", 
    \(\(DTest[T_, WP_] := 
        N[Abs[F[8, T] - GammaInTheLimit[8, T]], 
          WP];\)\), "\[IndentingNewLine]", 
    \(TInterval[Err_] := 
      x /. FindRoot[
          DTest[x, WP] \[Equal] Err, {x, {1, 50}}]\), "\[IndentingNewLine]", 
    \(\(ChebyshevRules[k_, 
          x_] := {a_Real\ x^m_ \[RuleDelayed] 
            a\ Pi*\(m!\)/\((2^
                        m\ \(\((\((m - k)\)/
                            2)\)!\)\ \(\((k + \((m - k)\)/
                              2)\)!\))\) /; \((m \[GreaterEqual] k && 
                  Mod[m - k, 2] \[Equal] 0)\), 
          a_Real\ x \[RuleDelayed] 
            a\ Pi*\(1!\)/\((2^1\ \(\((\((1 - k)\)/
                            2)\)!\)\ \(\((k + \((1 - k)\)/
                              2)\)!\))\) /; \((1 \[GreaterEqual] k && 
                  Mod[1 - k, 2] \[Equal] 0)\), 
          a_Real \[RuleDelayed] a\ Pi /; \((k \[Equal] 0)\), 
          a_Real\ x^m_ \[RuleDelayed] 
            0 /; Not[m \[GreaterEqual] k && Mod[m - k, 2] \[Equal] 0], 
          a_Real\ x \[RuleDelayed] 
            0 /; Not[1 \[GreaterEqual] k && Mod[1 - k, 2] \[Equal] 0], 
          a_Real \[RuleDelayed] 
            0 /; \((k =!= 0)\)};\)\), "\[IndentingNewLine]", 
    \(\(format[x_] := ToString[FixedNumberForm[x, 16, 2]];\)\)}], "Input"],

