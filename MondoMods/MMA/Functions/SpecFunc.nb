Cell["\<\

WP = 30;
NTerms =  4;
n = NTerms ;
switch = 28;

FunctionList[x_] := {Exp[-x]};
FuncList          = {\"Exp\"};

m=Length[FuncList];

ClearAll[Delta];

Delta = N[ ( (2^n (n+1)! MaxError / D[ F[0,-y],{y,n+1}])^(1/(n+1)) )/.y->( 0 \
) ,30];


NInterps = 1000;
Delta=N[switch/(2 NInterps+2),30 ];

Print[StringForm[\" Total Memory to Hold in Common = `` \", N[ ( n*m*NInterps \
word) * (16 bytes/word) * 1 Mb /(10^6 bytes) , 3 ] ]];
Print[StringForm[\" Max Error                      = `` \", format[N[ \
MaxError]]]];
Print[StringForm[\" Subdivision TWODEL             = \"],format[2*Delta] ];
Print[StringForm[\" Grid =                         = \"],format[1/(2*Delta)] \
];
Print[StringForm[\" Switch for Asymptoics, SWITCH  = \"],format[switch]];
Print[StringForm[\" Number of Interpolations       = \"],NInterps];

Do[

  FileName = StringJoin[FuncList[[m]],\".inc\"];
  Print[\"FileName = \",FileName];
  OpenWrite[FileName];

   ClearAll[b];

   time = SessionTime[];
   
(*----------------------------------------------------------------------------\
----------------------------------------------*)  

   Do[ 
      x1 = 2 j Delta ;
      x2 = x1+(2) Delta ;
      Int = N[ Normal[ Series[ FunctionList[(x1+x2)/2+(x2-x1)x/2][[m]] \
,{x,0,NTerms  +  30  }] ] , WP];
      Do[ a[i] = N[ (2/Pi) Int/.ChebyshevRules[i,x] , WP ] ,{i,0,NTerms}];
      PolyAnna = Expand[( a[0]/2  + Sum[ a[k] ChebyshevT[k,x] ,{k,1,NTerms}] \
)/.{x->(2*x-x1-x2)/(x2-x1)} ];
      Do[ b[i,j] = Coefficient[ PolyAnna , x^i ] , {i,NTerms} ];
      (* b[0,j] =  PolyAnna - Sum[ b[i,j] x^i ,{i,NTerms}] ; *)
      b[0,j] =  PolyAnna/.x->0;
     ,{j,0,NInterps}];
  
(*----------------------------------------------------------------------------\
----------------------------------------------*)  
  (*--------------------------------------------------------------------------\
------------------------------------------------*)  
  
  steps=21;
  div  = Floor[NInterps/steps];
     
  Do[
     Do[
        begin = j steps;
        end   = (j+1)steps -1;
        DataString=StringJoin[\"      DATA \
(\",FuncList[[m]],\"_\",ToString[i],\"(I),I=\",ToString[begin],\",\",ToString[\
end],\")/ & \\n\"];
        WriteString[FileName,DataString];
        Do[WriteString[FileName,StringJoin[\"      \",format[b[i,k+0]],\", \
\",format[b[i,k+1]], \", \",format[b[i,k+2]],\", & \\n\"]];
           ,{k,begin,end-4,3}];
        WriteString[FileName,StringJoin[\"      \",format[b[i,end-2]],\", \
\",format[b[i,end-1]], \", \",format[b[i,end]],\"/\\n\"]];
       ,{j,0,div-1}];
        begin = div steps;
        end   = NInterps;
        diff  = end-begin+1;
        DataString=StringJoin[\"      DATA \
(\",FuncList[[m]],\"_\",ToString[i],\"(I),I=\",ToString[begin],\",\",ToString[\
end],\")/ & \\n\"];
        WriteString[FileName,DataString];
        If[diff>=4,
           Do[WriteString[FileName,StringJoin[\"      \",format[b[i,k+0]],\", \
\",format[b[i,k+1]], \", \",format[b[i,k+2]],\", & \\n\"]];
              ,{k,begin,end-3,3}];
           ];
         If[ Mod[diff,3]==1,
             WriteString[FileName,StringJoin[\"      \",format[b[i,end]],\"/ \
\\n\"]];
            ,If[ Mod[diff,3]==2, 
                 WriteString[FileName,StringJoin[\"      \
\",format[b[i,end-1]],\", \",format[b[i,end]],\"/\\n\"]];
                ,If[ Mod[diff,3]==0, 
                      WriteString[FileName,StringJoin[\"      \
\",format[b[i,end-2]],\", \",format[b[i,end-1]], \", \
\",format[b[i,end]],\"/\\n\"]];
                    ];
                ];
            ];
    ,{i,0,NTerms}];
         
(*----------------------------------------------------------------------------\
----------------------------------------------*)  

(*----------------------------------------------------------------------------\
----------------------------------------------*)  
  
   Print[FuncList[[m]],StringForm[\" // Timing in hrs = \
``\",(SessionTime[]-time)/3600 ]];

   ,{m,Length[FuncList]}];
   
(*----------------------------------------------------------------------------\
----------------------------------------------*)  


Close[FileName];
\
\>", "Input",
