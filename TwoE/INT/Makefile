#
all:    INT
#
clean:  CINT
#
purge:clean
	rm -f $(MONDO_LIB)/libINT.a
	rm -f $(REMOVESRCE)
ppurge:purge
	rm -f *.Inc IntB*.F90 Ket*.F90 Bra*.F90 VRR*.F90 
#
source:
	math<INT.m>INT.out
	rm INT.out
#
INT:$(MiscObjs) $(IntObjs)
	$(AR) $(ARFLAGS) $(MONDO_LIB)/libINT.a $(?:.F90=.o)
	$(RANLIB) $(MONDO_LIB)/libINT.a
#
CINT:
	rm -f $(REMOVEMISC) $(REMOVEMODS)
	rm -f \#*
	rm -f *~
	ln -s /dev/null core
	ls -l
